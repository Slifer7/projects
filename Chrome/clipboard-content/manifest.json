{
  "name": "Clipboard Copier",
  "description": "Get data from clipboard",
  "version": "2.0",
  "permissions": [
    "activeTab",
    "tabs",
    "*://*/*",
    "clipboardRead",
    "storage",
    "contextMenus"
  ],
  "background": {
    "scripts": ["background.js"],
    "persistent": false
  },
  "browser_action": {
    "default_title": "Auto fill my info",
    "default_popup": "popup.html"
  },
  "manifest_version": 2,
  "options_page": "options.html"
}
