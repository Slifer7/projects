{
  "name": "Topic blocker",
  "description": "Topic blocker",
  "version": "2.0",
  "permissions": [
    "activeTab",
    "tabs",
    "webNavigation",
    "*://*/*"
  ],
  "background": {
    "scripts": ["background.js"],
    "persistent": false
  },
  "browser_action": {
    "default_title": "Auto fill my info"
  },
  "manifest_version": 2
}
