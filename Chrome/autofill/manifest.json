{
  "name": "Autofill",
  "description": "Auto fill a page",
  "version": "2.0",
  "permissions": [
    "activeTab",
    "tabs",
    "*://*/*",
    "webNavigation",
    "storage"
  ],
  "background": {
    "scripts": ["background.js"],
    "persistent": false
  },
  "browser_action": {
    "default_title": "Auto fill my info"
  },
  "manifest_version": 2,
   "options_page": "options.html"
}
